# ChainQuest Frontend Environment Variables

# Required: WalletConnect Project ID for wallet connections
# Get from: https://cloud.walletconnect.com
REACT_APP_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Optional: Alchemy API Key for better RPC performance
# Get from: https://alchemy.com
REACT_APP_ALCHEMY_API_KEY=your_alchemy_api_key_here

# Optional: Infura Project ID (alternative to Alchemy)
# Get from: https://infura.io
REACT_APP_INFURA_PROJECT_ID=your_infura_project_id_here

# Optional: Analytics and monitoring
REACT_APP_ANALYTICS_ID=your_analytics_id_here

# Development only - not needed for production
GENERATE_SOURCEMAP=false